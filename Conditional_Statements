// Problem 1: If the number is divisible by 3, print a "multiple of 3".

let number = 5;

if(number % 3 == 0){
  console.log("multiple of 3");
}
else{
  console.log("Not Multiple of 3")
}




// Problem 2: If a person is allowed to drive in India print "Apply for a license" or "NA".


let allowedAge = 18;
let personAge = 19;

if(personAge >= allowedAge){
  console.log("Apply for a license");
}
else{
  console.log("NA")
}




// Problem 3: Given 2 numbers a and b print which is greater or "both equal".

let a = 10;
let b = 8;

if(a > b){
  console.log("a is greater");
}
else if(b > a){
  console.log("b is greater");
}
else{
  console.log("both equal");
}






// Problem 4: Given stored username and password and input username and password, Print if the user can login or not.

let userName = "Mukesh";
let password = "mukesh@123";

let loginName = "Kumar";
let loginPassword = "mukesh@123";

if(userName === loginName && password === loginPassword){
  console.log("Login Success");
}
else{
  console.log("Please enter valid username and password")
}